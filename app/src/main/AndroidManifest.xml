<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.suitmedia.testapplicationsuitmedia">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".TestAppSuitmedia"
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.TestApplicationSuitmedia.NoActionBar"
        tools:targetApi="31">
        <activity
            android:name=".presentation.ui.user.SelectUser"
            android:exported="false" />
        <activity
            android:name=".presentation.ui.auth.LoginActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".presentation.ui.home.HomeActivity"
            android:exported="false" />
    </application>

</manifest>